class Solution {
    public int jump(int[] nums) {
        int pos = 0, des = 0;
        int count = 0;
        
        for(int i = 0;i< nums.length-1;i++){
            des = Math.max(des, i+nums[i]);
            if(pos == i){
                pos = des;
                count++;
            }
            
            
        }
        return count;
        
    }
}
