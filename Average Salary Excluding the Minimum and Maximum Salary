class Solution {
    public double average(int[] salary) {
        int n =  salary.length;
        double sum =0,result;
        int d = n-2;
        Arrays.sort(salary);
        for(int i = 1;i<n-1;i++){
            sum += salary[i];
        }
        
        result = sum/d;
        return result;
        
        
    }
}
