class Solution {
    public int lastStoneWeight(int[] stones) {
        Queue<Integer> q = new PriorityQueue<>(stones.length,Comparator.reverseOrder());
        for(int stone: stones){
            q.add(stone);
        }
        while(q.size()>= 2){
            int y = q.poll();
            int x = q.poll();
           // if(x == y) return 0;
             if(y>x){
                q.add(y-x);
            }
        }
        return q.isEmpty()?0 :q.peek();
        
    }
}
